n>\n\n    <b-button class=\"mt-3 ml-3\" block type=\"submit\" @click=\"$bvModal.hide('bv-modal-example')\" variant=\"primary\" >cancel</b-button>\n  </b-modal>\n\n\n        <div>\n          <b-form-input\n            v-model=\"text\"\n            placeholder=\"Search here\"\n            style=\"width: 200px; height: auto\"\n            class=\"mx-auto\"\n          ></b-form-input>\n          <!-- <div class=\"mt-2\">Value: {{ text }}</div> -->\n        </div>\n      </div>\n    </b-card>\n    <b-card style=\"width: 1400px; height: auto\" class=\"mx-auto\">\n      <div v-if=\"fruitlist.length > 0\">\n        <div class=\"pt-3 mx-auto\">\n          <b-table :items=\"fruitlist\" :fields=\"fields\">\n            <template v-slot:cell(name)=\"data\">\n              {{ data.item.name }}\n            </template>\n          </b-table>\n        </div>\n      </div>\n    </b-card>\n\n  </div>\n</template>\n<script>\n\n\nexport default {\n\n  data() {\n    return {\n      value1: '',\n      image: null,\n      fruitlist: [],\n      fields: [\n        { key: \"name\", label: \"ชื่อผลไม้\" },\n        { key: \"image\", label: \"\" },\n      ],\n      items: [{ name: \"John\", age: 30, image: \"../public/Red_Apple.jpg\" }],\n      text: \"\",\n    };\n  },\n  mounted(){\n    this.getfruit();\n  },\n  methods: {\n    getfruit() {\n      console.log(\"aaaaa\")\n      this.axios.get(\"http://localhost:3000\" + \"/getdatafruit\",{\n        headers: {\n          'Access-Control-Allow-Origin': '*',\n        }\n      }).then((response) =>{\n        console.log(\"fdsfsdf\")\n        console.log(response.data.data)\n        this.fruitlist=response.data.data;\n        console.log(this.fruitlist)\n      })\n    }\n\n  },\n  onSubmit() {\n    // extract the values from the form\n    let name = this.value1;\n    let photo = this.image;\n\n    // validate the input\n    if (!name || !photo) {\n      return this.$swal(\"Please provide a name and a photo\");\n    }\n\n    // make the HTTP request to the API endpoint\n    this.axios.post(\"http://localhost:3000\" + \"/upload\", {\n      name: name,\n      author: \"\",\n      photo: photo\n    }).then((response) => {\n      // handle the response from the API\n      if (response.message === \"Value and photo successfully added.\") {\n        this.$swal(\"Save Success\", \"\", \"success\").then(response => {\n          this.$bvModal.hide(\"bv-modal-example\");\n          // reload the page or fetch the updated data from the API\n          this.reloadpage();\n        });\n      }\n    });\n  }\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}